{
  "name": "dextools",
  "displayName": "dextools",
  "description": "Tools for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "publisher": "asambres",
  "repository": {
    "type": "git",
    "url": "https://github.com/sambres/dextools"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dextools.convert_doc_to_json"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dextools.convert_doc_to_json",
        "title": "Convert document to JSON",
        "description": "Change language mode to JSON and format document"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
